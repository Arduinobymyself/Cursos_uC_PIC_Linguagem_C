#include <Semafaro.h>

void main()
{

   while(TRUE)
   {
      //TODO: User Code
      output_high(pin_b0);
      output_low(pin_b1);
      output_low(pin_b2);
      delay_ms(3000);
      
      output_low(pin_b0);
      output_high(pin_b1);
      delay_ms(2000);
      
      output_low(pin_b1);
      output_high(pin_b2);
      delay_ms(1500);
      
      
   }

}
