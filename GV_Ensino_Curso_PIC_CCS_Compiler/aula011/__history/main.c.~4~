#include <main.h>

void main()
{

   while(TRUE)
   {
      output_b(0b0011111);
      delay_ms(300);
      output_b(0b00000110);
      delay_ms(300);
      output_b(0b01011011);
      delay_ms(300);
      output_b(0b01001111);
      delay_ms(300);
      output_b(0b01100110);
      delay_ms(300);
      output_b(0b01101101);
      delay_ms(300);
      output_b(0b0111101);
      delay_ms(300);
      output_b(0b00000111);
      delay_ms(300);
      output_b(0b0111111);
      delay_ms(300);
      output_b(0b01101111);
      delay_ms(300);
   }

}
